<body>
    <header id="header">
        <div class="container main-menu">
            <div class="row align-items-center justify-content-between d-flex">
                <div></div>
                <div id="logo">
                    <a href="/Home/Index"><img src="/Content/img/22_40.png" alt="" title="" /></a>
                </div>
                <div></div>

            </div>
        </div>
    </header>
    <section class="post-content-area single-post-area">
        <div class="container">
            <div class="comment-form form">
                <h4>ÜYE OL</h4>
                <form action="/User/Registration" class="form-group" method="post">
                    <div class="form-group col-lg-12s col-md-12">
                        <input type="text" class="form-control" name="adSoyad" placeholder="Name" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Name'">
                    </div>
                    <div class="form-group col-lg-12 col-md-12">
                        <input type="text" class="form-control" name="kullaniciAdi" placeholder="Username" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Username'">
                    </div>
                    <div class="form-group col-lg-12 col-md-12">
                        <input type="text" class="form-control" name="eMail" placeholder="Email Adress" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email Adress'">
                    </div>
                    <div class="form-group col-lg-12 col-md-12">
                        <input type="password" class="form-control" name="sifre" placeholder="Password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Password'">
                    </div>
                    <div>
                        <a>Zaten Üye Misin ?</a>
                        <a href="~/Security/Login">Giriş Yap</a>
                    </div>
                    <button style="margin-top:10px" class="primary-btn text-uppercase">KAYDOL</button>
                </form>
            </div>
        </div>
    </section>
    <script src="~/Content/js/vendor/jquery-2.2.4.min.js"></script>
